//Öz yineleme (RECURSİVE)

package javaapplication1;

public class JavaApplication1 {
    
    //1-10 arasındaki sayıların toplanması
    public static int f(int x){
        if(x == 10) return 10;
        return x + f(x+1);
    }
    
    public static void main(String[] args) {
        
        System.out.println(f(0));
                   
    }  
}
