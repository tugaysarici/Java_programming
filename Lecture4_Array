//Arrays (DİZİLER)

package javaapplication1;

public class JavaApplication1 {
    public static void main(String[] args) {
        int a[] = new int [5];
        // 0, 0, 0, 8, 0    yani atama yapmazsak Java sıfırlar atar.
        a[3] = 8;
        System.out.println(a[3]);
        int b[] = {1,3,5,7,11,13};
        String metin[] = {"Aynali","kemer","ince","bele"};
        System.out.println(metin[1]+b[3]+metin[3]);
    }  
}
